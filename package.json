{
  "name": "market-access-frontend",
  "version": "1.7.0",
  "description": "A front end for Market Access",
  "main": "server.js",
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  },
  "scripts": {
    "preversion": "npm test && npm run coverage",
    "postversion": "git push && git push --tags",
    "start": "NODE_ENV=production node --use-strict dist/server.js",
    "tag-master": "git tag v${npm_package_version}-master && git push --tags",
    "coverage": "NODE_ENV=development; LOG_LEVEL=error; SSO_BYPASS=true; JASMINE_CONFIG_PATH=src/test/app/jasmine.json nyc jasmine",
    "test": "NODE_ENV=development; LOG_LEVEL=error; SSO_BYPASS=true; JASMINE_CONFIG_PATH=src/test/app/jasmine.json jasmine",
    "test:health": "node src/test/ci/test-health.js",
    "test:setup": "cd src/test/ci/; ./setup.sh;",
    "test:features": "npm run test:setup; export TEST_BROWSER=chrome; npm run test:health && cucumber-js src/test/ci/features",
    "test:features:local": "npm run test:setup; export TEST_HOST=10.200.10.1; export TEST_BROWSER=chrome; cucumber-js src/test/ci/features",
    "watch": "run-p watch:*",
    "watch-stub": "export DATAHUB_STUB=true; npm run watch",
    "watch:node": "export LOG_LEVEL=debug; nodemon src/app/cluster.js --ignore 'src/public/**/*' --ignore 'src/test/**/*'",
    "grunt": "grunt dist",
    "build:dist": "npm run sass:prod && grunt dist && npm run dist-install",
    "dist-install": "cd dist && npm install --production",
    "dist": "npm run build:dist && npm run start",
    "heroku-postbuild": "npm run build:dist",
    "watch:sass": "npm run sass:dev && node-sass src/public/sass/main.scss src/public/css/main.css --source-map src/public/css/main.css.map --source-map-contents --include-path ./src/public/sass/ --include-path './src/@govuk-frontend/' -w -r",
    "sass:dev": "node-sass src/public/sass/main.scss src/public/css/main.css --source-map src/public/css/main.css.map --source-map-contents --include-path ./src/public/sass/ --include-path './src/@govuk-frontend/'",
    "sass:prod": "node-sass src/public/sass/main.scss src/public/css/main.css --include-path ./src/public/sass/ --include-path './src/@govuk-frontend/' -x --output-style compressed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uktrade/market-access-frontend.git"
  },
  "author": "Graham Veal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uktrade/market-access-frontend/issues"
  },
  "homepage": "https://github.com/uktrade/market-access-frontend#readme",
  "dependencies": {
    "compression": "1.7.2",
    "connect-redis": "3.3.3",
    "csurf": "1.9.0",
    "dateformat": "3.0.3",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "morgan": "1.9.0",
    "nunjucks": "3.1.2",
    "raven": "2.6.0",
    "redis": "2.8.0",
    "request": "2.85.0",
    "uuid": "3.2.1",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "axe-webdriverjs": "2.0.0",
    "cucumber": "4.2.1",
    "eslint": "4.19.1",
    "faker": "4.1.0",
    "grunt": "1.0.2",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "2.2.1",
    "grunt-contrib-uglify": "3.3.0",
    "grunt-filerev": "2.3.1",
    "grunt-mkdir": "1.0.0",
    "grunt-usemin": "3.1.1",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "json-schema-faker": "0.5.0-rc15",
    "load-grunt-tasks": "4.0.0",
    "nock": "9.3.0",
    "node-sass": "4.9.0",
    "nodemon": "1.17.4",
    "npm-run-all": "4.1.3",
    "nyc": "11.7.3",
    "proxyquire": "2.0.1",
    "selenium-webdriver": "4.0.0-alpha.1",
    "supertest": "3.0.0"
  }
}
