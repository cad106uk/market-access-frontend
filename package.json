{
  "name": "market-access-frontend",
  "version": "1.0.0",
  "description": "A front end for Market Access",
  "main": "server.js",
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  },
  "scripts": {
    "test": "NODE_ENV=development; LOG_LEVEL=error; JASMINE_CONFIG_PATH=src/test/app/jasmine.json jasmine",
    "test:health": "node src/test/ci/test-health.js",
    "start": "NODE_ENV=production node --use-strict dist/server.js",
    "watch": "export LOG_LEVEL=debug; nodemon src/app/cluster.js --ignore 'src/public/**/*' --ignore 'src/test/**/*'",
    "grunt": "grunt dist",
    "build:dist": "grunt dist && npm run dist-install",
    "dist-install": "cd dist && npm install --production",
    "dist": "npm run build:dist && npm run start",
    "heroku-postbuild": "npm run build:dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uktrade/market-access-frontend.git"
  },
  "author": "Graham Veal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uktrade/market-access-frontend/issues"
  },
  "homepage": "https://github.com/uktrade/market-access-frontend#readme",
  "dependencies": {
    "compression": "1.7.2",
    "express": "4.16.3",
    "morgan": "1.9.0",
    "nunjucks": "3.1.2",
    "raven": "2.6.0",
    "request": "2.85.0",
    "winston": "2.4.2"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "grunt": "1.0.2",
    "grunt-contrib-clean": "1.1.0",
    "grunt-contrib-concat": "1.0.1",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-cssmin": "2.2.1",
    "grunt-contrib-uglify": "3.3.0",
    "grunt-filerev": "2.3.1",
    "grunt-mkdir": "1.0.0",
    "grunt-usemin": "3.1.1",
    "jasmine": "3.1.0",
    "jasmine-core": "3.1.0",
    "load-grunt-tasks": "3.5.2",
    "nodemon": "1.17.3",
    "proxyquire": "2.0.1",
    "supertest": "3.0.0"
  }
}
