{
	"check-coverage": true,
	"lines": 90,
	"statements": 90,
	"functions": 90,
	"branches": 90,
	"include": [
		"src/app/**/*.js"
	],
	"exclude": [
		"src/app/app.js",
		"src/app/cluster.js",
		"src/app/config.js",
		"src/app/lib/datahub-request.stub.js",
		"src/app/**/*.spec.js"
	],
	"reporter": [
		"lcov",
		"text"
	],
	"cache": true,
	"all": true,
	"report-dir": "./coverage"
}
